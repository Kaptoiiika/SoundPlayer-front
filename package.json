{
  "name": "clientnew",
  "version": "1.0.0",
  "description": "",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack serve --env mode=development port=3000 apiURL=http://localhost",
    "build": "webpack --env mode=production apiURL=http://kapitoxa.ru",
    "build:dev": "webpack --env mode=development",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:fix": "npm run lint:ts:fix && npm run lint:scss:fix",
    "lint:ts": "npx eslint \"**/*{ts,tsx}\"",
    "lint:ts:fix": "npx eslint \"**/*{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "storybook": "start-storybook -p 6006 -c ./config/storybook",
    "build-storybook": "build-storybook -c ./config/storybook",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test --port 6006 ",
    "test:ui:ci": "npm run build-storybook && npx loki --requireReference --reactUri file:./storybook-static",
    "test:ui:ok": "npx loki approve",
    "test:ui:report": "npm run test:ui:report:json && npm run test:ui:report:html",
    "test:ui:report:json": "node scripts/generate-visual-json-report.js",
    "test:ui:report:html": "npx reg-cli --from .loki/report.json --report .loki/report.html",
    "prepare": "husky install",
    "gen": "node ./scripts/createSlice/index.js"
  },
